<?xml version="1.0"?>
<extension key="wp-media-library" type="module">
  <file>wp_media_library</file>
  <name>wp-media-library</name>
  <description>Use of Wordpress mdeia in Civicrm</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>Richard Turner</name>
      <email>richard@antipode.info</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Main Extension Page">https://none.com</url>
    <url desc="Documentation">https://none.com</url>
    <url desc="Support">https://none.com</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-01-22</releaseDate>
  <version>1.0.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.81</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/WpMediaLibrary</namespace>
    <format>24.09.1</format>
    <angularModule>crmWpMediaLibrary</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
